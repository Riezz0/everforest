{

/////General/////////////////////////////////////////////////////////////////////////////////////////////////|
"height": 30, "spacing": 4, "margin-left": 5, "margin-right": 5, "margin-top": 5, "margin-bottom": 2,      //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Modules Placement///////////////////////////////////////////////////////////////////////////////////////|
"modules-left": ["custom/logo","custom/salaat","custom/updates"],                                          //|
"modules-center": ["hyprland/workspaces"],                                                                 //|
"modules-right": ["custom/spacer3","pulseaudio","custom/spacer2","network","custom/spacer",                //| 
"clock","tray","custom/power"],		               				                           //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|


/////Workspaces//////////////////////////////////////////////////////////////////////////////////////////////|
"hyprland/workspaces": {                                                                                   //|
"disable-scroll": true,                                                                                    //|
"all-outputs": true,                                                                                       //|
"warp-on-scroll": false,                                                                                   //|
"format": "{icon}",                                                                                        //|
"format-icons": {                                                                                          //|
"active": "<span font='20px'>󰮯</span>",                                                                    //|
"default": "<span font='20px'>󰊠</span>"}},                                                                 //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Power///////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/power": {                                                                                          //|
    "format": "<span font='25px'>󰐦</span>",								   //|
    "on-click": "python ~/.config/scripts/power.py"},                                                      //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Spacer-1////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/spacer": {                                                                                         //|
    "format": "<span font='25px'>󰥔</span>",                                                                //|
    "interval": "once",                                                                                    //|
    "tooltip": false},                                                                                     //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

////Spacer-2/////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/spacer2": {                                                                                        //|
    "format": "<span font='25px'>󰈀</span>",                                                                //|
    "interval": "once",                                                                                    //|
    "tooltip": false},                     								   //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

////Spacer-3/////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/spacer3": {                        								   //|
    "format": "<span font='25px'>󰋋</span>",								   //|
    "interval": "once",                    								   //|
    "tooltip": false},                     								   //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

////Spacer-4/////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/spacer4": {                        								   //|
    "format": "<span font='25px'></span>",								   //|
    "interval": "once",                    								   //|
    "tooltip": false},                     								   //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////AUR/////////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/updates": {											   //|
"format": "{} {icon}",    										   //|
"return-type": "json",										           //|
"format-icons": {											   //|
"has-updates": "<span font='20px'>󱝣</span>",								   //|
"updated": "<span font='20px'>󱝍</span>"},								   //|
"on-click": "~/.config/scripts/updates.sh",								   //|
"exec-if": "which waybar-module-pacman-updates",							   //|
"exec": "waybar-module-pacman-updates --interval-seconds 5 --network-interval-seconds 300"},		   //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Tray////////////////////////////////////////////////////////////////////////////////////////////////////|
"tray": {"icon-size": 21, "spacing": 10},                                                                  //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Clock///////////////////////////////////////////////////////////////////////////////////////////////////|
"clock": {                                                                                                 //|  
"tooltip": false,											   //|
"format": "{:%H:%M:%S}",                                                                                   //|
"interval": 1 },							                                   //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Network/////////////////////////////////////////////////////////////////////////////////////////////////|
"network": {                                                                                               //|
        "format-ethernet": "{ipaddr}",                                                                     //|
	"tooltip": false,                                                                                  //|
        "format-disconnected": "Disconnected ⚠"},                                                          //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Arch-Logo///////////////////////////////////////////////////////////////////////////////////////////////|
"custom/logo": {                                                                                           //|
    "format": "<span font='25px'>󰣇</span>",                                                                //|
    "on-click": "rofi -show drun",                                                                         //|
    "tooltip": false},                                                                                     //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Arch-Logo///////////////////////////////////////////////////////////////////////////////////////////////|
"custom/salaat": {                                                                                         //|
    "exec": "python ~/.config/scripts/salaat.py",                                                          //|
    "return-type": "json",                                                                                 //|
    "tooltip": true,											   //|
    "interval": 3 },                                                                                       //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|



/////Audio///////////////////////////////////////////////////////////////////////////////////////////////////|
    "pulseaudio": {                                                                                        //|
        "scroll-step": 1, // %, can be a float                                                             //|
        "format": "{volume}%",                                                                             //|
	"tooltip": false,                                                                                  //|
        "on-click": "pavucontrol"}                                                                         //|
}////////////////////////////////////////////////////////////////////////////////////////////////////////////|
