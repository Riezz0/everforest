{

/////General/////////////////////////////////////////////////////////////////////////////////////////////////|
"height": 30, "spacing": 4, "margin-left": 5, "margin-right": 5, "margin-top": 5, "margin-bottom": 2,      //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Modules Placement///////////////////////////////////////////////////////////////////////////////////////|
"modules-left": ["custom/logo"],                                                                           //|
"modules-center": ["hyprland/workspaces"],                                                                 //|
"modules-right": ["custom/spacer3","pulseaudio","custom/spacer2","network","custom/spacer","clock","tray"],//|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|


/////Workspaces//////////////////////////////////////////////////////////////////////////////////////////////|
"hyprland/workspaces": {                                                                                   //|
"disable-scroll": true,                                                                                    //|
"all-outputs": true,                                                                                       //|
"warp-on-scroll": false,                                                                                   //|
"format": "{icon}",                                                                                        //|
"format-icons": {                                                                                          //|
"active": "<span font='20px'>󰮯</span>",                                                                    //|
"default": "<span font='20px'>󰊠</span>"}},                                                                 //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Spacer-1////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/spacer": {                                                                                         //|
    "format": "<span font='25px'>󰥔</span>",                                                                //|
    "interval": "once",                                                                                    //|
    "tooltip": false},                                                                                     //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

////Spacer-2/////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/spacer2": {                                                                                        //|
    "format": "<span font='25px'>󰈀</span>",                                                                //|
    "interval": "once",                                                                                    //|
    "tooltip": false},                     								   //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

////Spacer-3/////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/spacer3": {                        								   //|
    "format": "<span font='25px'>󰋋</span>",								   //|
    "interval": "once",                    								   //|
    "tooltip": false},                     								   //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Tray////////////////////////////////////////////////////////////////////////////////////////////////////|
"tray": {"icon-size": 21, "spacing": 10},                                                                  //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Clock///////////////////////////////////////////////////////////////////////////////////////////////////|
"clock": {                                                                                                 //|  
"tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",                               //|
"format": "{:%H:%M:%S}",                                                                                   //|
"interval": 1,								                                   //|
"format-alt": "{:%Y-%m-%d}"},						                                   //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Network/////////////////////////////////////////////////////////////////////////////////////////////////|
"network": {                                                                                               //|
        "format-ethernet": "{ipaddr}",                                                                     //|
	"tooltip": false,                                                                                  //|
        "format-disconnected": "Disconnected ⚠"},                                                          //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|

/////Spacer-1////////////////////////////////////////////////////////////////////////////////////////////////|
"custom/logo": {                                                                                           //|
    "format": "<span font='25px'>󰣇</span>",                                                                //|
    "on-click": "~/.config/rofi/launcher/launcher.sh",                                                     //|
    "tooltip": false},                                                                                     //|
/////////////////////////////////////////////////////////////////////////////////////////////////////////////|



/////Audio///////////////////////////////////////////////////////////////////////////////////////////////////|
    "pulseaudio": {                                                                                        //|
        "scroll-step": 1, // %, can be a float                                                             //|
        "format": "{volume}%",                                                                             //|
	"tooltip": false,                                                                                  //|
        "on-click": "pavucontrol"}                                                                         //|
}////////////////////////////////////////////////////////////////////////////////////////////////////////////|
